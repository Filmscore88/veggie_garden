
<h2> Gardens</h2>
<table>
  <tr>
    <th>Name</th>
    <th>Planting System</th>
    <th>Link</th>
  </tr>
  <%@gardens.each do |garden|%>
  <tr>
    <td><%=garden.name%></td>
    <td><%=garden.planting_system%></td>
    <td><a href= "/gardens/<%=garden.id%>">View</a></td>
  <%end%>
  </tr>
</table>
