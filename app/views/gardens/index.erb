<h1>All Gardens</h1>
<ul>
  <% @gardens.each do |garden| %>

    <li><%= garden.name %></li>
    <li><%= garden.planting_system %></li>
    <li><%=garden.id%></li>
    <% if garden.farmer_id == current_user.id %>
      <li><a href= "/gardens/<%=garden.id%>/edit">Edit</a></li>
    <%end%>
  <% end %>
</ul>
