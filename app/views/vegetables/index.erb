<h1>All Vegetables</h1>
<ul>
  <% @vegetables.each do |veggie| %>
    <li><%= veggie.name %></li>
    <li><%= veggie.quantity%></li>

      <%current_user.gardens.each do |garden|%>
        <%if veggie.garden_id == garden.id%>
        <li><a href= "/vegetables/<%=veggie.id%>/edit">Edit</a></li>
       <%end%>
    <%end%>
  <%end%>
</ul>
